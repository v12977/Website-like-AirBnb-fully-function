
<% layout("/layouts/boilerplate") %>

<div class="row mt-3">
    <h1 class="col-6 offset-3">Login Page!</h1>
    <div class="col-6 offset-3">
        <form action="/login" method="POST" class="needs-validation" novalidate>
        <div class="mb-2">
        <label for="username" class="form-lebel">Username</label>
        <input name="username" id="username" type="text" class="form-control" required />
        </div>
        
        <div class="mb-2">
        <label for="password" class="form-lebel">Password</label>
        <input name="password" id="password" type="text" class="form-control" required />
        </div>
        <button class="btn btn-dark">Login!</button>
         </form>
    </div>
 </div>